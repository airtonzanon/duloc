[alias]
    d = diff
    ds = diff --stat
    dw = diff --color-words
    l = log
    lo = log --oneline
    lp = log --patch
    lpw = log --patch --color-words
    last = log --stat --max-count 1
    ls = log --stat
    cme = !git commit --message=\"$(emoji-random)\"
    st = status
[user]
    name = Henrique Moody
    email = henriquemoody@gmail.com
[color]
    branch = auto
    diff = auto
    status = auto
    ui = auto
[core]
    pager = less
    editor = vim
    excludesfile = ~/.gitignore
[push]
    default = current
[rebase]
    autosquash = true
[gc]
    auto = 0
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true
